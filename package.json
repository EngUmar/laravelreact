        "cross-env": "^5.1",
        "jquery": "1.12.4",
        "laravel-mix": "^2.0",
        "lodash": "^4.17.4",
        "popper.js": "^1.12",
        "react": "^16.2.0",
        "react-dom": "^16.2.0",
        "bootstrap-select": "1.9.4"
    },
    "dependencies": {
        "history": "^4.7.2",
